@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Website.Models.Auth.LoginInfo
@{
	Layout = "~/Views/Shared/_Layout.cshtml";

		if(Model!=null){
			if (Model.EmailPlainText == null) Model.EmailPlainText = string.Empty;
			if (Model.PasswordPlainText == null) Model.EmailPlainText = string.Empty;
		}
}
<div class="container row">
	<div class="col col-md-4"></div>
	<div class="col col-md-4">
		<div class="display-4 text-center p-1">Join PRACC</div>

	<partial name="~/Views/Shared/PartialViews/User/_RegisterFormPartialView.cshtml" model=Model />

	</div>
	<div class="col col-md-4"></div>
</div>