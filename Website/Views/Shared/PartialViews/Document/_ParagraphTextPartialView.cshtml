@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Website.Models.DocumentModel.DocumentParagraph

@if (Model.TextParts is not null)
{
	foreach (var webtext in Model.TextParts)
	{
		<div class="text-wrap">
			<partial name="~/Views/Shared/PartialViews/Document/_WebTextPartialView.cshtml" model=webtext />
		</div>
	}
}

