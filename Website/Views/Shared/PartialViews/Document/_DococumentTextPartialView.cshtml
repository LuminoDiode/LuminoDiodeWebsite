@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Website.Models.DocumentModel.Document



@{
	if (Model.TextPrerenderedHtml is not null)
	{
		Html.Encode(Model.TextPrerenderedHtml);
	}
	else
	{
		foreach (var paragraph in Model.Paragraphs)
		{
			<partial name="~/Views/Shared/PartialViews/Document/_ParagraphTextPartialView.cshtml" model=paragraph />
		}
	}
}

